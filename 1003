import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        Scanner entrada = new Scanner (System.in);
        int A;
        int B;
        int SOMA;
        
        A = entrada.nextInt();
        B = entrada.nextInt();
        SOMA = A+B;
        System.out.println("SOMA = " +SOMA);
    }
    
}
